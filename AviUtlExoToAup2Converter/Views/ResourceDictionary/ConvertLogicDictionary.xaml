                        <Border Visibility="{Binding HasEnd, Converter={StaticResource bool2VisibilityConverter}}">
                            <ContentPresenter Content="{Binding End}" ContentTemplateSelector="{StaticResource convertLogicTemplateSelector}" Margin="5" VerticalAlignment="Center"/>
                        </Border>
                        <Border Visibility="{Binding NoEnd, Converter={StaticResource bool2VisibilityConverter}}"
                                Background="Yellow"
                                Style="{StaticResource NoItemBorder}">
                            <TextBlock Text="終了位置を設定してください" Margin="5" HorizontalAlignment="Center"/>
                        </Border>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                        <TextBlock Text="再生範囲" VerticalAlignment="Center"/>
                        <Border Visibility="{Binding HasRange, Converter={StaticResource bool2VisibilityConverter}}">
                            <ContentPresenter Content="{Binding Range}" ContentTemplateSelector="{StaticResource convertLogicTemplateSelector}" Margin="5" VerticalAlignment="Center"/>
                        </Border>
                        <Border Visibility="{Binding NoRange, Converter={StaticResource bool2VisibilityConverter}}"
                                Background="Cyan"
                                Style="{StaticResource NoItemBorder}">
                            <TextBlock Text="範囲を設定してください" Margin="5" HorizontalAlignment="Center"/>
                        </Border>
                    </StackPanel>
                </StackPanel>
            </StackPanel>
        </Border>
    </DataTemplate>
