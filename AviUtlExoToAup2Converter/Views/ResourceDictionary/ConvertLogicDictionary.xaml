                    <i:Interaction.Behaviors>
                        <vb:DropBehavior Command="{Binding DropOperand2Command}"/>
                    </i:Interaction.Behaviors>
                    <TextBlock Text="条件2を設定してください" Margin="5" HorizontalAlignment="Center"/>
                </Border>
                <TextBlock Text="のとき" HorizontalAlignment="Center"/>
            </StackPanel>
        </Border>
    </DataTemplate>
