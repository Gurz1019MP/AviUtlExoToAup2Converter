<Window x:Class="AviUtlExoToAup2Converter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AviUtlExoToAup2Converter"
        xmlns:v="clr-namespace:AviUtlExoToAup2Converter.Views"
        xmlns:vm="clr-namespace:AviUtlExoToAup2Converter.ViewModels"
        xmlns:vmie="clr-namespace:AviUtlExoToAup2Converter.ViewModels.Item.Exo"
        xmlns:vmia="clr-namespace:AviUtlExoToAup2Converter.ViewModels.Item.Aup2"
        mc:Ignorable="d"
        Title="AviUtlExoToAup2Converter" Height="450" Width="800">
    <Window.DataContext>
        <vm:MainViewModel/>
    </Window.DataContext>
    <TabControl>
        <TabItem Header="変換">
            <v:Convert DataContext="{Binding Convert}"/>
        </TabItem>
        <TabItem Header="設定">
            <v:Setting DataContext="{Binding Setting}"/>
        </TabItem>
    </TabControl>
</Window>
