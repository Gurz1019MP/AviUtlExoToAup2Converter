            </i:Interaction.Behaviors>
            <StackPanel Orientation="Horizontal">
                <TextBox Text="{Binding Model.To}" VerticalAlignment="Center" Margin="5,0"
                            Style="{StaticResource TransparentTextBox}"/>
                <TextBlock Text="へ次の値を設定" VerticalAlignment="Center"/>
                <StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Border Visibility="{Binding HasStart, Converter={StaticResource bool2VisibilityConverter}}">
                            <ContentPresenter Content="{Binding Start}" ContentTemplateSelector="{StaticResource convertLogicTemplateSelector}" Margin="5" VerticalAlignment="Center"/>
                        </Border>
                        <Border Visibility="{Binding NoStart, Converter={StaticResource bool2VisibilityConverter}}"
                                Background="Yellow"
                                Style="{StaticResource NoItemBorder}">
                            <TextBlock Text="開始位置を設定してください" Margin="5" HorizontalAlignment="Center"/>
